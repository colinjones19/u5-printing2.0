        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:U5Printing"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="copies"                Height="25" TextWrapping="Wrap" Text="How Many Copies?" Margin="91,39,-91,255"/>
        <Button x:Name="price" Content="Calculate" Width="200" Click="Pricefinder" Margin="158,50,159,0" Height="51"/>
    </Grid>
</Window>
